<!DOCTYPE html>
<html>
<head>
  <meta charset="UTF-8" />
  <title>ZBoard</title>
  <link href="http://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
  {% block stylesheets %}
  <link rel="stylesheet" href="{{ asset('assets/vendor/Materialize/dist/css/materialize.min.css') }}" />
  {% endblock %}
  <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
</head>

<body>
  <nav>
    <div class="nav-wrapper" style="background-color:#36127d;">
      <a href="#" class="brand-logo">ZBoard</a>
      <ul id="nav-mobile" class="right hide-on-med-and-down">
        <li><a href=".html">Accueil</a></li>
        <li><a href=".html">Mes mentorés</a></li>
        <li><a href=".html">Mon compte</a></li>
        <li><a href=".html">Déconnexion</a></li>
      </ul>
    </div>
  </nav>

  {% block body %}{% endblock %}

  <footer class="page-footer" style="background-color:#FFFFFF;">
    <div class="footer-copyright" style="background-color:#36127d;">
      <div class="container" style="background-color:#36127d;">
        © 2016 Copyright Nathanaël Langlois, Guillaume Loulier et Christophe Lablancherie
        <a class="grey-text text-lighten-4 right" href="#!">Nous contacter</a>
      </div>
    </div>
  </footer>

  {% block javascripts %}
  <script src="{{ asset('assets/vendor/jquery/dist/jquery.min.js') }}"></script>
  <script src="{{ asset('assets/vendor/Materialize/dist/materialize.min.js') }}"></script>
  <script src="{{ asset('js/main.js') }}"></script>
  {% endblock %}
</body>

</html>
